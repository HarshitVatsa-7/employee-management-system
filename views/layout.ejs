<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <title><%= typeof title !== 'undefined' ? title : 'EMS' %></title>
  <link rel="stylesheet" href="/public/css/style.css" />
  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
</head>
<body>
  <%- include('partials/header') %>
  <main class="container">
    <% if (success_msg) { %><div class="alert success"><%= success_msg %></div><% } %>
    <% if (error_msg) { %><div class="alert error"><%= error_msg %></div><% } %>
    <%- body %>
  </main>
  <%- include('partials/footer') %>
</body>
</html>
